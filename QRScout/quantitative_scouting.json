            "9072": "9072"
          }
        },
        {
          "title": "Starting Position",
          "description": "The starting position of the robot.",
          "type": "select",
          "required": false,
          "code": "Prsp",
          "formResetBehavior": "reset",
          "defaultValue": "",
          "choices": {
            "R1": "Outpost Side",
            "R2": "Middle",
            "R3": "Depot Side"
          }
        },
        {
          "title": "No Show",
          "description": "Check if the robot did not show up for the match.",
          "type": "boolean",
          "required": false,
          "code": "noShow",
          "formResetBehavior": "reset",
          "defaultValue": false
        }
      ]
    },
    {
      "name": "Autonomous",
      "fields": [
        {
          "title": "Type of Auto",
          "description": "Check which zone(s) the robot intaked during auto.",
          "type": "multi-select",
          "required": false,
          "code": "typeAuto",
          "formResetBehavior": "reset",
          "choices": {
            "1": "Depot",
            "2": "Outpost/HP",
            "3": "Neutral"
          }
        },
        {
          "title": "Bump",
          "description": "Check if the robot went over the bump at least once during auto.",
          "type": "boolean",
          "required": false,
          "code": "bumpAuto",
          "formResetBehavior": "reset",
          "defaultValue": false
        },
        {
          "title": "Climb",
          "description": "Check if the robot climbed during auto.",
          "type": "boolean",
          "required": false,
          "code": "climbAuto",
          "formResetBehavior": "reset",
          "defaultValue": false
        },
        {
          "title": "Amount of Cycles (Tenths)",
          "description": "How much fuel the robot held compared to the total possible fuel it can hold.",
          "type": "range",
          "required": false,
          "code": "hopcapAuto",
          "formResetBehavior": "reset",
          "defaultValue": 0,d
          "min": 0,
          "max": 20,
          "step": 1
        }
      ]
    },
    {
      "name": "Teleop",
      "fields": [
        {
          "title": "Trench",
          "description": "Chech if the robot went under the trench.",
          "type": "counter",
          "required": false,
          "code": "trenchTele",
          "formResetBehavior": "reset",
          "defaultValue": 0,
          "min": 0,
          "max": 1000,
          "step": 1
        },
        {
          "title": "Bump",
          "description": "Check if the robot went over the bump.",
          "type": "counter",
          "required": false,
          "code": "bumpTele",
          "formResetBehavior": "reset",
          "defaultValue": 0,
          "min": 0,
          "max": 1000,
          "step": 1
        },
        {
          "title": "Number of Cycles",
          "description": "Count number of cycles the robot completed.",
          "type": "counter",
          "required": false,
          "code": "numCycles",
          "formResetBehavior": "reset",
          "defaultValue": 0,
          "min": 0,
          "max": 1000,
          "step": 1
        },
        {
          "title": "Amount of Cycles (Tenths)",
          "description": "..",
          "type": "range",
          "required": false,
          "code": "cycles",
          "formResetBehavior": "reset",
          "defaultValue": 0,
          "min": 0,
          "max": 9,
          "step": 1
        }
      ]
    },
    {
      "name": "Endgame",
      "fields": [
        {
          "title": "Climbed Tower",
          "description": "Check if the robot was able to climb and to which level.",
          "type": "select",
          "required": true,
          "code": "climbLevel",
          "formResetBehavior": "reset",
          "defaultValue": "No",
          "choices": {
            "No": "Didn't Climb",
            "L1": "Level 1",
            "L2": "Level 2",
            "L3": "Level 3",
            "F": "Failed Climb"
          }
        },
        {
          "title": "Climb <5 sec",
          "description": "Check if the robot climbed in less than five seconds.",
          "type": "boolean",
          "required": false,
          "code": "climbTime",
          "formResetBehavior": "reset",
          "defaultValue": false
        }
      ]
    },
    {
      "name": "Postmatch",
      "fields": [
        {
          "title": "Inactive Strategy",
          "description": "Strategy the robot played during their inactive shifts.",
          "type": "multi-select",
          "required": false,
          "code": "inactiveStrat",
          "formResetBehavior": "reset",
          "choices": {
            "1": "Pass",
            "2": "Thief",
            "3": "Defense(OZ)",
            "4": "Defense(NZ)",
            "5": "Intaked Full"
          }
        },
        {
          "title": "Driver Rating",
          "description": "How well did the driver perform.",
          "type": "range",
          "required": false,
          "code": "driverRating",
          "formResetBehavior": "reset",
          "defaultValue": 1,
          "min": 1,
          "max": 5,
          "step": 1
        },
        {
          "title": "Died?",
          "type": "boolean",
          "required": false,
          "code": "died",
          "formResetBehavior": "reset",
          "defaultValue": false
        },
        {
          "title": "Solo Shooting",
          "description": "Robot shot by itself for more than 3 seconds?",
          "type": "boolean",
          "required": false,
          "code": "soloShooting",
          "formResetBehavior": "reset",
          "defaultValue": false
        },
        {
          "title": "Yellow/Red Card",
          "type": "select",
          "required": true,
          "code": "card",
          "formResetBehavior": "reset",
          "defaultValue": "No Card",
          "choices": {
            "No Card": "No Card",
            "Yellow": "Yellow Card",
            "Red": "Red Card"
          }
        },
        {
          "title": "Comments",
          "type": "text",
          "required": false,
          "code": "comments",
          "formResetBehavior": "reset",
          "defaultValue": "",
          "min": 0,
          "max": 100
        }
      ]
    }
  ]
